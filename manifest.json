{
  "manifest_version": 2,
  "name": "page headline",
  "version": "1.0",
  
  "browser_action": {
    "default_icon": "icon_19.png",
    "default_popup": "popup.html"
  },

  "permissions": ["http://*/*", "https://*/*"],

  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": [ "jquery-1.12.0.min.js",
            "content.js"
    ]
  }]

}